<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DAL"
                   namespace="DAL.com._9jahealth.data.models">

  <class name="GeneratedPINDetail" table="GeneratedPINDetail">
    <id name="GeneratedPINDetailId" column="Id" >
      <generator class="identity" />
    </id>

    <property name="GeneratedBatchNo" column="generatedBatchNo" />
    <property name="PinValue" column="pinValue" type="StringClob" />
    <property name="Status" column="status" />
    <property name="IsActived" column="isActivated" />
    <property name="Cost" column="cost" />
    <property name="TSDate" column="tsDate" />
    <property name="SessionId" column="sessionId" type="StringClob" />

  </class>
</hibernate-mapping>
