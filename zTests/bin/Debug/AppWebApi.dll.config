<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  https://go.microsoft.com/fwlink/?LinkId=301879
  -->
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  
  <connectionStrings>
    <add name="db_local" connectionString="Data Source=.\sqlexpress;Database=Health9;Initial Catalog=Health9;Trusted_Connection=yes" providerName="System.Data.SqlClient"/>
    <add name="db_stag" connectionString="Data Source=...;Database=Health9;Initial Catalog=Health9;Trusted_Connection=no;User Id=app_usr;Password=user123" providerName="System.Data.SqlClient"/>
    <add name="db_dev" connectionString="Data Source=...;Database=Health9;Initial Catalog=Health9;Trusted_Connection=no;User Id=app_usr;Password=user123" providerName="System.Data.SqlClient"/>
    <add name="db_prod" connectionString="Data Source=...;Database=Health9;Initial Catalog=Health9;Trusted_Connection=no;User Id=app_usr;Password=user123" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  

  <log4net>
    <!-- The Logs file appender -->
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="AppendToFile" value="true" />
      <param name="DatePattern" value="yyyy.MM.dd" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <maximumFileSize value="100MB" />
      <file type="log4net.Util.PatternString">
        <conversionPattern value="Logs\Dashboard_%processid_%date{yyyy.MMM.dd}.log" />
      </file>
      <filter type="log4net.Filter.LoggerMatchFilter">
        <loggerToMatch value="NHibernate.SQL" />
        <acceptOnMatch value="false" />
      </filter>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <conversionPattern value="%date %-5level [%thread] l4n-requestor=%property{requestor} l4n-requesttimestamp=%property{requesttimestamp} l4n-transactionid=%property{transactionId} l4n-env=%property{logger_environment} l4n-logger=%logger aspnet-net_sessionid=%aspnet-request{ASP.NET_SessionId} aspnet-ms_sessionid=%aspnet-request{ms_sessionid} aspnet-request_method=%aspnet-request{REQUEST_METHOD} aspnet-server_name=%aspnet-request{SERVER_NAME} aspnet-url=%aspnet-request{URL} aspnet-query_string=%aspnet-request{QUERY_STRING} [%property{logger_data}] %message%exception%newline" />
      </layout>
    </appender>

    <!-- TODO: We'll probably eventually want to change this to INFO rather than DEBUG for production -->
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFile" />
    </root>

  </log4net>


  <appSettings>
    <add key="ENVIRONMENT" value="localhost" />
    <add key="IncludeStackTrace" value="false" />

    <add key="ActiveConnectionString" value="db_local" />

  </appSettings>
  
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.7.2" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.7.2" />
    <httpRuntime targetFramework="4.7.1" />
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.4.0" newVersion="5.2.4.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.codedom>
    <compilers>
      <compiler language="c#;cs;csharp" extension=".cs" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.CSharpCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.8.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" warningLevel="4" compilerOptions="/langversion:default /nowarn:1659;1699;1701" />
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.VBCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.8.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" warningLevel="4" compilerOptions="/langversion:default /nowarn:41008 /define:_MYTYPE=\&quot;Web\&quot; /optionInfer+" />
    </compilers>
  </system.codedom>
</configuration>